{
  "cli": {
    "version": ">= 16.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "FIREBASE_API_KEY": "env",
        "FIREBASE_AUTH_DOMAIN": "env",
        "FIREBASE_PROJECT_ID": "env",
        "FIREBASE_STORAGE_BUCKET": "env",
        "FIREBASE_MESSAGING_SENDER_ID": "env",
        "FIREBASE_APP_ID": "env",
        "MEASUREMENT_ID": "env"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal"
      }
    }
  }
}
